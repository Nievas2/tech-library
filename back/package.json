{
  "name": "open-library",
  "version": "1.0.0",
  "description": "Backend del proyecto de libreria ",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "start": "tsc && node dist/server.js",
    "start:dev": "tsc && concurrently \"tsc -w\" \"nodemon dist/server.js\"",
    "start:prod": "SET NODE_ENV=production && npm start",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/config/data.source.ts",
    "m:gen": "npm run typeorm migration:generate",
    "m:run": "npm run typeorm migration:run",
    "db:create": "npm run m:gen -- src/migrations/CreateDb && npm run m:run"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.10.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
